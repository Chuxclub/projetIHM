<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="inventoryVBox" prefHeight="400.0" prefWidth="165.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainPanelView">
    <children>
        <Label fx:id="inventoryLabel" text="Inventory" />
        <ScrollPane fx:id="inventoryScrollPane" fitToWidth="true" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
            <content>
                <AnchorPane fx:id="inventoryScrollPaneAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
            </content>
        </ScrollPane>
        <FlowPane fx:id="buttonFlowPane" alignment="CENTER" prefHeight="16.0" prefWidth="97.0">
            <children>
                <Button fx:id="useButton" mnemonicParsing="false" text="Use" />
                <Button fx:id="giveButton" mnemonicParsing="false" text="Give" />
                <Button fx:id="dropButton" mnemonicParsing="false" text="Drop" />
            </children>
            <VBox.margin>
                <Insets top="5.0" />
            </VBox.margin>
        </FlowPane>
    </children>
    <HBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </HBox.margin>
</VBox>