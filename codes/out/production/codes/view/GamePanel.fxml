<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="slhButton" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" style="-fx-border-color: blue;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.GamePanelController">
    <children>
        <Button fx:id="saveButton" mnemonicParsing="false" text="Save" />
        <Button fx:id="helpButton" mnemonicParsing="false" onMouseClicked="#printHelp" text="?" />
        <Button fx:id="loadButton" mnemonicParsing="false" text="Load" />
    </children>
    <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </VBox.margin>
</HBox>